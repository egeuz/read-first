{
  "manifest_version": 2,
  "name": "Read First",
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": [
        "http://www.facebook.com/*",
        "https://www.facebook.com/*"
      ],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]

  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup/index.html",
    "default_title": "Read First v.0.0.1"
  }
}
